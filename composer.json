{
  "name": "ahmed-aliraqi/laravel-adminlte",
  "description": "An easy way to integrate AdminLTE into your laravel applications.",
  "keywords": ["laravel", "adminlte", "dashboard", "admin", "administrator"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmed Fathy",
      "email": "aliraqi.dev@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/support": "~5.6|^6.0",
    "illuminate/console": "~5.6|^6.0",
    "laracasts/flash": "^3.0",
    "davejamesmiller/laravel-breadcrumbs": "^5.3"
  },
  "repositories": [
        {
            "type": "composer",
            "url":  "https://github.com/ahmed-aliraqi/laravel-adminlte"
        }
    ],
  "autoload": {
    "psr-4": {
      "AhmedAliraqi\\Adminlte\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "AhmedAliraqi\\Adminlte\\AdminlteServiceProvider"
      ]
    }
  }
}
